{
  "expo": {
    "name": "TrainMe",
    "slug": "trainme",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "assetBundlePatterns": ["**/*"],
    "web": {
      "port": 3000,
      "bundler": "metro"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#070B1A"
      },
      "enableSound": false
    },
    "ios": {
      "supportsTablet": true,
      "enableSound": false
    }
  }
}
